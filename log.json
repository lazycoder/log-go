{
    "level": "trace",
    "appenders": [
        {
            "type": "ConsoleAppender",
            "level": "trace",
            "layout": "(level)|(file):(line)|(text)"
        },
        {
            "type": "RollingFileAppender",
            "async": true,
            "path": "/tmp/log_test/(time|20060102)/(time|15).log",
            "level": "trace",
            "layout": "(time|2006-01-02 15:04:05)|(level)|(file):(line)|(text)"
        },
        {
            "type": "MongoDBAppender",
            "async": true,
            "db": "192.168.10.11/log_test",
            "collection": "(level)",
            "layout":
            {
                "time":     "(time|2006-01-02#15:04:05)",
                "level":    "(level)",
                "location": "(file):(line)",
                "text":     "(text)"
            }
        }
    ]
}
